{"code":"import * as getTagNames from './utils/getTagNames';\r\nimport hyphenate from './utils/hyphenate';\r\nimport lookupComponent from './lookupComponent';\r\nfunction parseStoryComponent(story) {\r\n    if (!story.template && !story.render) {\r\n        throw new Error('`template` or `render` must be on component options, but got undefined.');\r\n    }\r\n    var tagNames = story.template\r\n        ? getTagNames.fromTemplate(story.template).map(hyphenate)\r\n        : getTagNames.fromJSX(story.render);\r\n    var components = tagNames\r\n        .map(function (tagName) {\r\n        return lookupComponent(tagName, story.components);\r\n    })\r\n        .filter(function (info) { return !!info; });\r\n    return components;\r\n}\r\nexport default parseStoryComponent;\r\n","dts":{"name":"/home/pockawoooh/development/github.com/pocka/storybook-addon-vue-info/parseStoryComponent.d.ts","text":"import ComponentInfo from './types/ComponentInfo';\r\nimport { RuntimeComponentOptions } from './types/VueRuntime';\r\ndeclare function parseStoryComponent(story: RuntimeComponentOptions): ComponentInfo[];\r\nexport default parseStoryComponent;\r\n"}}
